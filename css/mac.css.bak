/**设置公共样式**/
body,p,h1,h2,h3,h4,h5,h6,ul,li,dl,dt,dd{
	margin:0px;
	padding:0px;
	list-style:none;
}
body{
	font-size:12px;
	font-family:"微软雅黑";
}
body,#header,#search,#banner,#section,#footer,#search{
	width:1200px;
	margin:0px auto;
}
#header{
	height:45px;
	line-height:45px;
	background-color:#363636;
}
#header>ul{
	width:1000px;
	height:45px;
	line-height:45px;
	margin:0px auto;
}
#header>ul>li{
	float:left;
	width:125px;
	line-height:45px;
}
#header>ul>li>a{
	text-decoration:none;
	color:#fff;
}
#header>ul>li>a:hover{
	color:#ddd;
}
#header>ul>li.current>a{
	color:#ddd;
}
/**遮罩层**/
#search{
	height:45px;
	line-height:45px;
	width:1200px;
	background-color:#363636;
	position:relative;
	top:0px;
	z-index:10;
	margin:0px auto;
	display:none;
}
#search>.content>p{
	float:left;
	margin-left:100px;
}
#search input{
	float:left;
	width:600px;
	line-height:30px;
	margin-left:100px;
	border:0px;
	padding:0px;
	outline:none;
	margin-top:7px;
	background:#363636 url('../img/search.png') no-repeat 0px 5px;
	padding-left:28px;
	font-size:16px;
	color:#999999;
}
#search>.content>a{
	float:left;
	text-decoration:none;
	font-size:16px;
	color:#999999;
	margin-left:127px;
}
#link{
	width:630px;
	background-color:#fff;
	position:absolute;
	top:45px;
	left:220px;
	padding:0px 20px 20px 20px;
	color:#9F9F9F;
}
#link p{
	clear:both;
	font-size:12px;
}
#link li{
	font-size:14px;
	height:30px;
	line-height:30px;
	color:#9F9F9F;
	padding-left:20px;
	cursor:pointer;
}

#link li:hover{
	color:#0A8CCE;
	background-color:#eee;
}
/***购物车部分***/
#header li.go{
	position:relative;
}
#header li.go>b{
	display:inline-block;
	width:0px;
	height:0px;
	border:8px solid transparent;
	border-bottom-color:#D6D6D6;
	position:absolute;
	top:37px;
	left:5px;
	z-index:11;
	display:none;
}
#go_box{
	width:290px;
	padding:10px 0px;
	background-color:#fff;
	position:absolute;
	top:53px;
	left:-127px;
	z-index:10;
	border:1px solid #D6D6D6;
	display:none;
}
#go_box>span{
	display:inline-block;
	line-height:30px;
	width:300px;
	color:#A1A1A1;
	font-size:14px;
	text-align:center;
}
#go_box>ul>li>img{
	width:25px;
	height:25px;
	position:relative;
	top:8px;
	left:0px;
	z-index:20;
}
#go_box>ul>li{
	line-height:45px;
	margin:5px 15px;
	border-top:1px solid #E3E3E3;
}
#go_box>ul>li>a{
	text-decoration:none;
	color:#2C9CD4;
}
/*底部内容*/
#footer{
	background-color:#F2F2F2;
	margin-top:5px;
	padding:10px 0px 30px 0px;
	overflow:hidden;
}

.msg{
	width:1000px;
	overflow:hidden;
	margin:0px auto;
	text-align:center;
}
.msg ul{
	width:200px;
	line-height:20px;
	float:left;
}
.msg ul li a{
	text-decoration:none;
	color:#6A6A77;
}
.msg ul li a:hover{
	text-decoration:underline;
	color:#000;
}
.msg ul li b{
	color:#333333;
}
#footer p.xg{
	width:900px;
	margin-left:140px;
	margin-top:20px;
	padding-bottom:5px;
	color:#A9A199;
	border-bottom:1px solid #E3E3E3;
}
#footer p a{
	text-decoration:none;
	color:#6291CC;
}
#footer p a:hover{
	text-decoration:underline;
	color:#000;
}
#footer p.left{
	float:left;
	margin-left:140px;
	margin-top:10px;
	color:#A9A199;
}
#footer div.right{
	float:left;
	margin-top:10px;
	margin-left:30px;
	color:#A9A199;
	margin-bottom:10px;
}
#footer div.right a{
	text-decoration:none;
	color:#666;
	padding:0px 10px;
}
#footer p.last{
	clear:both;
	margin-left:140px;
	color:#A9A199;
}
#footer div.right a:hover{
	text-decoration:underline;
	color:#000;
}
/***主体内容****/
#section{
	height:112px;
	background-color:#F6F6F6;
}
#img_nav{
	clear:both;
	width:1000px;	
	margin-left:150px;
	overflow:hidden;
}
#img_nav>dl{
	float:left;
	margin:0px 10px;
	text-align:center;
	cursor:pointer;
}
#img_nav a{
	text-decoration:none;
	color:#000;
}
#img_nav a:hover{
	color:#aaa;
}
/**************************************产品介绍*************************************************/
#product{
	width:1200px;
	overflow:hidden;
}
#product .one,#product .three{
	width:1200px;
	overflow:hidden;
}
#product .one img,#product .three img{
	float:left;
}

#product .one div{
	float:left;
	width:400px;
	margin-top:350px;
	text-align:center;
}
#product .three div{
	float:left;
	margin-left:-300px;
	margin-top:150px;
	text-align:center;
}
#product .one div>span{
	padding:5px 15px;
	border:1px solid #F46C0E;
	border-radius:5px;
}
#product .one div>h1,#product .three div>h1,#product .four div>h1{
	font-size:50px;
	margin-top:20px;
	color:#333333;
	font-weight:normal;
}
#product .one div>h2,#product .three div>h2,#product .four div>h2{
	color:#333339;
	margin-bottom:10px;
	font-weight:normal;
}
#product .one div>a,#product .three div>a,#product .four div>a{
	font-size:14px;
	font-weight:normal;
	text-decoration:none;
	color:#4F88CF;
}
#product .one div>a:hover,#product .three div>a:hover,#product .four div>a:hover{
	text-decoration:underline;
}
#product .one div>b,#product .three div>b，#product .four div>b{
	color:#4F88CF;
}
#product .two{
	width:1200px;
	height:650px;
	background:url("../img/mac_06.jpg") no-repeat 200px 0px;
}
#product .two div{
	float:left;
	width:400px;
	margin-top:200px;
	margin-left:100px;
	text-align:center;
}
#product .two div>h1{
	font-size:50px;
	margin-top:20px;
	color:#333333;
	font-weight:normal;
}
#product .two div>h2{
	color:#333339;
	margin-bottom:10px;
	font-weight:normal;
}
#product .two div>a{
	font-size:14px;
	font-weight:normal;
	text-decoration:none;
	color:#4F88CF;
}
#product .two div>a:hover{
	text-decoration:underline;
}
#product .two div>b{
	color:#4F88CF;
}
#product .four>img{
	float:right;
}
#product .four>div{
	float:left;
	width:400px;
	text-align:center;
	margin-top:-800px;
}
#compare{
	width:1200px;
	margin-top:100px;
	overflow:hidden;
	text-align:center;
	padding-bottom:100px;
}
#compare>.title{
	text-align:center;
}
#compare>.title>h1{
	font-size:40px;
	color:#333333;
	font-weight:normal;
	margin-bottom:10px;
}
#compare>.title>a{
	font-size:16px;
	font-weight:normal;
	text-decoration:none;
	color:#4F88CF;
}
#compare>.title>a:hover{
	text-decoration:underline;
}
#compare>.content{
	margin-top:20px;
}
#compare>.content>.all{
	margin-top:60px;
	width:1200px;
	border-top:1px solid #D6D6D6;
	padding:60px 0px 110px 0px;
}
#compare>.content>.all>.left{
	float:left;
	width:600px;
	text-align:center;
}
#compare>.content>.all>.left>h1,#compare>.content>.all>.right>h1{
	font-size:18px;
	color:#595959;
	font-weight:normal;
}
#compare>.content>.all>.left>p,#compare>.content>.all>.right>p{
	line-height:25px;
	font-size:14px;
	color:#595959;
}
#compare>.content>.all>.left>a,#compare>.content>.all>.right>a{
	margin-top:10px;
	color:#4F88CF;
	text-decoration:none;
}
#compare>.content>.all>.left>a:hover,#compare>.content>.all>.right>a:hover{
	text-decoration:underline;
}
#compare>.content>.all>.right{
	float:left;
	width:600px;
	text-align:center;
}
#app{
	width:1200px;
	text-align:center;
	margin-bottom:110px;
}
#app>.title{
	width:800px;
	margin:0px auto;
}
#app>.title>h1{
	font-size:40px;
	color:#333333;
	font-weight:normal;
	margin-bottom:10px;
}
#app>.title>p{
	font-size:14px;
	color:#333333;
	line-height:25px;
}
#app>.title>p>a{
	color:#4F88CF;
	text-decoration:none;
}
#app>.title>p>a:hover{
	text-decoration:underline;
}
#app>.title>ul{
	width:700px;
	margin:0px auto;
	overflow:hidden;
	margin-top:50px;
}
#app>.title>ul>li{
	float:left;
	width:100px;
	padding-bottom:10px;
	text-align:center;
	border-bottom:1px solid #D6D6D6;
	cursor:pointer;
}
#app>.title>ul>li.com{
	border-bottom-color:#000;
}
#a1{
	margin-top:40px;
	width:1200px;
	overflow:hidden;
}
#a2{
	margin-top:40px;
	width:1200px;
	overflow:hidden;
	display:none;
}
#a3{
	margin-top:40px;
	width:1200px;
	overflow:hidden;
	display:none;
}
#a4{
	margin-top:40px;
	width:1200px;
	overflow:hidden;
	display:none;
}
#a1>img,#a2>img,#a3>img,#a4>img{
	float:left;
}
#a1>.right,#a2>.right,#a3>.right,#a4>.right{
	float:left;
	margin-left:80px;
	width:400px;
	text-align:left;
}
#a1>.right>h1,#a2>.right>h1,#a3>.right>h1,#a4>.right>h1{
	color:#333333;
	margin-top:40px;
	font-weight:normal;
	font-size:22px;
}
#a1>.right>p,#a2>.right>p,#a3>.right>p,#a4>.right>p{
	color:#333333;
	margin-top:30px;
	font-size:14px;
	margin-bottom:10px;
}
#a1>.right>img,#a2>.right>img,#a3>.right>img,#a4>.right>img{
	margin-top:30px;
}
#a1>.right a,#a2>.right a,#a3>.right a,#a4>.right a{
	color:#4F88CF;
	text-decoration:none;
}
#a1>.right a:hover,#a2>.right a:hover,#a3>.right a:hover,#a4>.right a:hover{
	text-decoration:underline;
}